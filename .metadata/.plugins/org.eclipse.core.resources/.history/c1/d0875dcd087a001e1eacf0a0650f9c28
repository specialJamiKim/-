package classTest;

class Order{
	String orderNo;
	String orderID;
	String orderDate;
	String name;
	int orderItemNumber;
	String address;

	void showOrderInfo() {
		System.out.printf("주문번호:%l, 주문자아이디:%s, 주문날짜:%s,주문자 이름:%s, 주문상품번호:%d, 배송주소:%s",orderNo,orderID,orderDate,name,orderItemNumber,address);
	}
	
	
}

public class OrderTest {


	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Order or = new Order();
		
		or.orderNo = "20180312001";
		or.orderID = "abc123";
		or.orderDate = "2018년3월12일";
		
		
	}

}
