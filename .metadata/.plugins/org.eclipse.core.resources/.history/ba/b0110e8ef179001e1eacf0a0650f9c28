package classTest;

public class Person {
	String name;
	int height;
	int weight;
	char gender; 
	boolean married;
	
	public void showStudentInfo(){
		System.out.println(name + "," +height + "," + weight + "," + gender + "," + married);
	}
	

	 Person(){
		 this("undefined",0,0,'x',true)
		 
	 }

	
	Person(String name, int height, int weight,char gender, boolean married){
		this.name = name;
		this.height = height;
		this.weight = weight;
		this.gender = gender;
		this.married = married;
	}
	
	public String toString() {
		return name + "," + height + ","+ weight + ","+ gender + "," + married;
	}
}
