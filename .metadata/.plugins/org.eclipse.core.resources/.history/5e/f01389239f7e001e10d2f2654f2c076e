package cooperation;

public class TakeTrans {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Student s1 = new Student("김태룡",4,10000);
		Student s2 = new Student("토마스",3,10000);
		Bus b = new Bus(179);
		Subway s = new Subway(1);
		
		s1.takeBus(b);
		s1.takeSubway(s);
		s1.showInfo();
		
		s2.takeSubway(s);
		s2.showInfo();
		
	}

}
