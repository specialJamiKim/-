package cooperation;

public class Student {
	public String StudentName;
	public int grade;
	public int money;
	
	public Student() {
		this("홍길동",2,10000);
	}
	
	public Student(String StudentName, int grade, int money)
	{
		this.StudentName = StudentName;
		this.grade = grade;
		this.money = money;
	}
}
