package classTest;

public class FunctionTest {
	
	//1부터 파라미터로 받아온 값까지 더해주는 메서드
	
	static int addMe(int x) {
		int sum = 0;
		for(int i = 1; i <= x ; i++)
			sum +=i;
		
		return sum;
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stu
		
		System.out.println(FunctionTest.addMe(10));
	}

}
