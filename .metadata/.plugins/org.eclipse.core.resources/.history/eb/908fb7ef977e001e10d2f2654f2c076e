package thistest;

class Birthday{
	int day;
	int month;
	int year;
	public int getDay() {
		return day;
	}
	public void setDay(int day) {
		this.day = day;
	}
	public int getMonth() {
		return month;
	}
	public void setMonth(int month) {
		this.month = month;
	}
	public int getYear() {
		return year;
	}
	public void setYear(int year) {
		this.year = year;
	}
	
	public Birthday(){
		this(1900,00,00);
	}
	
	public Birthday returnItself() {
		return this;
	}
	
	public Birthday(int year, int month, int day) {
		this.year = year;
		this.month = month;
		this.day = day;
		System.out.println(this.getYear());
	}
	
	public String toString() {
		return year + "/" + month + "/" + day;
	}
}

public class ThisTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Birthday my = new Birthday();
		System.out.println(my);
	}

}
